//第一题
#include<iostream>
using namespace std;
int main(){
    cout << "Hello,World!" << endl;
}

//第二题
#include<iostream>
using namespace std;
int main(){
   
    cout << "  * " << endl;
    cout << " *** " << endl;
    cout << "***** " << endl;
    cout << " *** " << endl;
    cout << "  * " << endl;
    return 0;
}

//第三题
#include<iostream>
using namespace std;

int main(){
        cout << "                ********" << endl;
        cout << "               ************" << endl;
        cout << "               ####....#." << endl;
        cout << "             #..###.....##...." << endl;
        cout << "             ###.......######              ###            ###" << endl;
        cout << "                ...........               #...#          #...#" << endl;
        cout << "               ##*#######                 #.#.#          #.#.#" << endl;
        cout << "            ####*******######             #.#.#          #.#.#" << endl;
        cout << "           ...#***.****.*###....          #...#          #...#" << endl;
        cout << "           ....**********##.....           ###            ###" << endl;
        cout << "           ....****    *****...." << endl;
        cout << "             ####        ####" << endl;
        cout << "           ######        ######" << endl;
        cout << "##############################################################" << endl;
        cout << "#...#......#.##...#......#.##...#......#.##------------------#" << endl;
        cout << "###########################################------------------#" << endl;
        cout << "#..#....#....##..#....#....##..#....#....#####################" << endl;
        cout << "##########################################    #----------#" << endl;
        cout << "#.....#......##.....#......##.....#......#    #----------#" << endl;
        cout << "##########################################    #----------#" << endl;
        cout << "#.#..#....#..##.#..#....#..##.#..#....#..#    #----------#" << endl;
        cout << "##########################################    ############" << endl;
        return 0;
}

//第四题
#include<iostream>
using namespace std;

int main(){
    int a, b;
    cin >> a >> b;
    cout << a + b << endl;
    return 0;
}

//第五题
#include<iostream>
using namespace std;
int main(){
    char s;
    cin >> s;
    cout << "  " << s << endl;
    cout << " " << s << s << s << endl;
    cout << s << s << s << s << s << endl;
    return 0;
}

//第六题
#include<iostream>
using namespace std;
int main(){
    int n, k;
    cin >> n >> k;
    cout << n * k << endl;
    return 0;
}

//第七题
#include<iostream>
using namespace std;
int main(){
    char a, b;
    cin >> a;
    b = a - 32;
    cout << b << endl;
    return 0;
}
